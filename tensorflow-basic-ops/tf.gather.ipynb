    "import numpy as np\n",
    "import tensorflow as tf\n",
    " \n",
    "params = tf.constant(np.reshape(np.arange(30), [5, 6]))\n",
    "indices = tf.constant([0, 2, 3])\n",
    "selected = tf.gather(params, indices)\n",
    "\n",
    "sess = tf.Session()\n",
    "print(sess.run(params))\n",
    "print(sess.run(selected))"
   ]
