   "source": [
    "import tensorflow as tf\n",
    "\n",
    "X = tf.random_normal([20, 1, 10])\n",
    "output = tf.squeeze(X, axis=1)\n",
    "\n",
    "sess = tf.Session()\n",
    "out = sess.run(output)\n",
    "print(out.shape)"
